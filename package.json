{
	"name": "octave-hacking",
	"displayName": "Octave Hacking",
	"description": "Support for GNU Octave programming",
	"publisher": "apjanke",
	"author": {
		"name": "Andrew Janke",
		"email": "floss@apjanke.net"
	},
	"version": "0.2.15-SNAPSHOT",
	"homepage": "https://github.com/apjanke/vscode-octave-hacking",
	"repository": {
		"type": "git",
		"url": "https://github.com/apjanke/vscode-octave-hacking"
	},
	"bugs": {
		"url": "https://github.com/apjanke/vscode-octave-hacking/issues"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"icon": "images/octave-hacking-logo.png",
	"activationEvents": [
		"onLanguage:octave"
	],
	"main": "./out/main.js",
	"contributes": {
		"languages": [
			{
				"id": "octave",
				"aliases": [
					"Octave"
				],
				"extensions": [
					".m"
				],
				"filenames": [
					"PKG_ADD",
					"PKG_DEL"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "octave",
				"scopeName": "source.octave",
				"path": "./syntaxes/Octave.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "octave",
				"path": "./snippets.json"
			}
		]
	},
	"scripts": {
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "build": "npm run esbuild-base -- --sourcemap",
    "build-watch": "npm run esbuild-base -- --sourcemap --watch",
    "fmt": "prettier --write \"src/**/*.ts\"&& npm run test -- --fix",
    "test": "eslint src --ext ts && tsc --noEmit",
		"compile": "tsc -p ./",
    "test-compile": "tsc -p ./",
		"pretest": "npm run compile",
		"test-old": "node ./out/test/runTest.js",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.7",
		"@types/node": "^20.14.10",
		"@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
		"@vscode/test-electron": "^2.4.1",
		"esbuild": "^0.23.0",
		"eslint": "^8.56.0",
		"glob": "^9.0.3",
		"log-symbols": "^5.1.0",
		"mkdirp": "^3.0.1",
		"mocha": "^10.6.0",
		"object-inspect": "^1.13.2",
		"path-parse": "^1.0.7",
		"prettier": "^3.3.2",
		"side-channel": "^1.0.6",
		"sprintf-js": "^1.1.3",
		"typescript": "^5.5.3"
	},
	"dependencies": {
		"ansi-colors": "^4.1.3",
		"camelcase": "^8.0.0",
		"diff": "^5.2.0",
		"es-abstract": "^1.23.3",
		"es-to-primitive": "^1.2.1",
		"growl": "^1.10.5",
		"http-proxy-agent": "^7.0.2",
		"https-proxy-agent": "^7.0.5",
		"once": "^1.4.0",
		"rimraf": "^5.0.8",
		"semver": "^7.6.2",
		"set-blocking": "^2.0.0",
		"types": "^0.1.1"
	}
}
